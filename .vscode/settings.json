{
    "files.exclude": {
        "include": true,
        "node_modules": true,
        "**/package-lock.json": true,
        "**/package.json": true,
        "**/.*": false,
        "**/.gitignore": false,
        ".vscode/**": false,
        ".venv": true,
        "**/__pycache__": true
    },
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "python.analysis.typeCheckingMode": "basic",
    "[html]": {
        "editor.tabSize": 2,
        "editor.suggest.insertMode": "replace"
    },
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace"
    },
    "rest-client.environmentVariables": {
        "local": {
            "host": "localhost",
        }
    },
    "sqltools.connections": [
        {
            "mysqlOptions": {
                "authProtocol": "default",
                "enableSsl": "Disabled"
            },
            "previewLimit": 50,
            "server": "localhost",
            "port": 3306,
            "driver": "MariaDB",
            "name": "2024proc5",
            "database": "2024proc5",
            "username": "2024proc5_admin"
        }
    ]
}